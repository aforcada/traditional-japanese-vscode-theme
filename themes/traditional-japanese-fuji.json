  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6a7d90"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#f1d9a5"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "support.constant"],
      "settings": {
        "foreground": "#ebb08c"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#6ba6d6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#7ec4dd"
      }
    },
    {
      "scope": ["entity.name.type", "support.type", "support.class"],
      "settings": {
        "foreground": "#8fd2be"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#6ba6d6"
      }
    },
    {
      "scope": ["variable.other.constant", "constant.other"],
      "settings": {
        "foreground": "#b6d0e6"
      }
    },
    {
      "scope": ["entity.name.tag", "support.type.property-name"],
      "settings": {
        "foreground": "#a99be6"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#b6d0e6"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#b5c0cc"
      }
    },
    {
      "scope": "meta.brace",
      "settings": {
        "foreground": "#b5c0cc"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#ffffff",
        "background": "#b33e5c"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#b6d0e6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#7ec4dd",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#a99be6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.link",
      "settings": {
        "foreground": "#8fd2be"
      }
    }
  ],
  "semanticTokenColors": {
    "variable.readonly": "#b6d0e6",
    "variable.defaultLibrary": "#8fd2be",
    "property": "#a99be6",
    "function": "#7ec4dd",
    "method": "#7ec4dd",
    "string": "#f1d9a5",
    "number": "#ebb08c",
    "enumMember": "#b6d0e6"
  }
}
