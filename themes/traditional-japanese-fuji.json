  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#768ba0"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "support.constant"],
      "settings": {
        "foreground": "#f0ba94"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#86cde3"
      }
    },
    {
      "scope": ["entity.name.tag", "support.type.property-name"],
      "settings": {
        "foreground": "#b0a3ef"
      }
    },
    {
      "scope": ["entity.name.type", "support.type", "support.class"],
      "settings": {
        "foreground": "#8bd3be"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.attribute-name",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#d6c9f2"
      }
    },
    {
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#8bd3be"
      }
    },
    {
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#8f87ca"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element"
      ],
      "settings": {
        "foreground": "#a9a0e4"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#ffffff",
        "background": "#ba4563"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#75b0de",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#86cde3",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#bdd6ec",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#b0a3ef",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.link",
      "settings": {
        "foreground": "#8bd3be"
      }
    },
    {
      "scope": "meta.brace",
      "settings": {
        "foreground": "#bcc7d4"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#bdd6ec"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#bcc7d4"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#f5dfad"
      }
    },
    {
      "scope": "support.constant.property-value",
      "settings": {
        "foreground": "#f0ba94"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#75b0de"
      }
    },
    {
      "scope": ["variable.other.constant", "constant.other"],
      "settings": {
        "foreground": "#bdd6ec"
      }
    }
  ],
  "semanticTokenColors": {
    "enumMember": "#bdd6ec",
    "function": "#86cde3",
    "method": "#86cde3",
    "number": "#f0ba94",
    "property": "#b0a3ef",
    "string": "#f5dfad",
    "variable.defaultLibrary": "#8bd3be",
    "variable.readonly": "#bdd6ec"
  }
}
