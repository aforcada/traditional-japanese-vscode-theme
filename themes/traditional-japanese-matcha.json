  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#7c826f"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#f0d6a4"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "support.constant"],
      "settings": {
        "foreground": "#d2aa5c"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#8ccf58",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#9ddc7a"
      }
    },
    {
      "scope": ["entity.name.type", "support.type", "support.class"],
      "settings": {
        "foreground": "#74c6a0"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#8ccf58"
      }
    },
    {
      "scope": ["variable.other.constant", "constant.other"],
      "settings": {
        "foreground": "#c9d891"
      }
    },
    {
      "scope": ["entity.name.tag", "support.type.property-name"],
      "settings": {
        "foreground": "#8bcf58"
      }
    },
    {
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#d6b280"
      }
    },
    {
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#74c6a0"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.attribute-name",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#c7e3a6"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element"
      ],
      "settings": {
        "foreground": "#a9d68f"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#b7dd8e"
      }
    },
    {
      "scope": "support.constant.property-value",
      "settings": {
        "foreground": "#d2aa5c"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#c1cbb4"
      }
    },
    {
      "scope": "meta.brace",
      "settings": {
        "foreground": "#c1cbb4"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#ffffff",
        "background": "#b9654c"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#c9d891",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#9ddc7a",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#74c6a0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.link",
      "settings": {
        "foreground": "#8dd68f"
      }
    }
  ],
  "semanticTokenColors": {
    "variable.readonly": "#c9d891",
    "variable.defaultLibrary": "#74c6a0",
    "property": "#b7dd8e",
    "function": "#9ddc7a",
    "method": "#9ddc7a",
    "string": "#f0d6a4",
    "number": "#d2aa5c",
    "enumMember": "#c9d891"
  }
}
