  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#8a6a5c"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#f0c871"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "support.constant"],
      "settings": {
        "foreground": "#d98a55"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#c7352b",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#d97a57"
      }
    },
    {
      "scope": ["entity.name.type", "support.type", "support.class"],
      "settings": {
        "foreground": "#82a668"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#c7352b"
      }
    },
    {
      "scope": ["variable.other.constant", "constant.other"],
      "settings": {
        "foreground": "#d9a327"
      }
    },
    {
      "scope": ["entity.name.tag", "support.type.property-name"],
      "settings": {
        "foreground": "#bc5e7f"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#d9a327"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#c19a86"
      }
    },
    {
      "scope": "meta.brace",
      "settings": {
        "foreground": "#c19a86"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#ffffff",
        "background": "#c7352b"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#d9a327",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#d97a57",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#bc5e7f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.link",
      "settings": {
        "foreground": "#82a668"
      }
    }
  ],
  "semanticTokenColors": {
    "variable.readonly": "#d9a327",
    "variable.defaultLibrary": "#82a668",
    "property": "#bc5e7f",
    "function": "#d97a57",
    "method": "#d97a57",
    "string": "#f0c871",
    "number": "#d98a55",
    "enumMember": "#d9a327"
  }
}
