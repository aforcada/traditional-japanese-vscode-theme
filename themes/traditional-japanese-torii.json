  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#8a6a5c"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#f0c871"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "support.constant"],
      "settings": {
        "foreground": "#d98a55"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#e24c2a",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#eb7a55"
      }
    },
    {
      "scope": ["entity.name.type", "support.type", "support.class"],
      "settings": {
        "foreground": "#8fba72"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#e24c2a"
      }
    },
    {
      "scope": ["variable.other.constant", "constant.other"],
      "settings": {
        "foreground": "#e8b64c"
      }
    },
    {
      "scope": ["entity.name.tag", "support.type.property-name"],
      "settings": {
        "foreground": "#d26d82"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#e8b64c"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#c19a86"
      }
    },
    {
      "scope": "meta.brace",
      "settings": {
        "foreground": "#c19a86"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#ffffff",
        "background": "#e24c2a"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#e8b64c",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#eb7a55",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#d26d82",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.link",
      "settings": {
        "foreground": "#8fba72"
      }
    }
  ],
  "semanticTokenColors": {
    "variable.readonly": "#e8b64c",
    "variable.defaultLibrary": "#8fba72",
    "property": "#d26d82",
    "function": "#eb7a55",
    "method": "#eb7a55",
    "string": "#f0c871",
    "number": "#d98a55",
    "enumMember": "#e8b64c"
  }
}
