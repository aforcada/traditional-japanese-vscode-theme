  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#a47c6b"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "support.constant"],
      "settings": {
        "foreground": "#e08654"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#f27c56"
      }
    },
    {
      "scope": ["entity.name.tag", "support.type.property-name"],
      "settings": {
        "foreground": "#d84d42"
      }
    },
    {
      "scope": ["entity.name.type", "support.type", "support.class"],
      "settings": {
        "foreground": "#f0a25e"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.attribute-name",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#8ab173"
      }
    },
    {
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#f0a25e"
      }
    },
    {
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#d27a8c"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element"
      ],
      "settings": {
        "foreground": "#f0c253"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#ffffff",
        "background": "#d6453a"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#d84d42",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#f27c56",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#f0c253",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#d27a8c",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.link",
      "settings": {
        "foreground": "#f0c253"
      }
    },
    {
      "scope": "meta.brace",
      "settings": {
        "foreground": "#d8b49f"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#f0c253"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#d8b49f"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#f6d07c"
      }
    },
    {
      "scope": "support.constant.property-value",
      "settings": {
        "foreground": "#e08654"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#d84d42"
      }
    },
    {
      "scope": ["variable.other.constant", "constant.other"],
      "settings": {
        "foreground": "#f0c253"
      }
    }
  ],
  "semanticTokenColors": {
    "enumMember": "#f0c253",
    "function": "#f27c56",
    "method": "#f27c56",
    "number": "#e08654",
    "property": "#f27c56",
    "string": "#f6d07c",
    "variable.defaultLibrary": "#f6ce6d",
    "variable.readonly": "#f0c253"
  }
